mponents/Footer';\nimport FilterCard from '../../components/FilterCard';\nimport TaskCard from '../../components/TaskCard';\nfunction Home() {\n\n  //Variavél de Estado\n  const [filterActived,setFilterActived]=useState('all');\n  const [tasks,setTasks]=useState([]);\n  const [lateCount,setLateCount]=useState();\n\n  async function loadTasks(){\n    await api.get(`/task/filter/${filterActived}/11:11:11:11:11:11`)\n    .then(response=>{\n        setTasks(response.data)\n    });\n  }\n\n\n  async function lateVerify(){\n    await api.get(`/task/filter/late/11:11:11:11:11:11`)\n    .then(response=>{\n        setLateCount(response.data.length)\n    });\n  }\n\n  function Notification(){\n    setFilterActived('late');\n  }\n\n    useEffect(()=>{\n      loadTasks();\n      lateVerify();\n    },[filterActived])\n\n    \n  return (\n      <S.Container>\n        <Header lateCount={lateCount} clickNotification={Notification}/>\n      <S.FilterArea>\n        <button type=\"button\" onClick={()=>setFilterActived(\"all\")}><FilterCard title=\"Todos\" actived={filterActived==='all'}/></button>\n        <button type=\"button\" onClick={()=>setFilterActived(\"today\")}><FilterCard title=\"Hoje\" actived={filterActived==='today'}/></button>\n        <button type=\"button\" onClick={()=>setFilterActived(\"week\")}><FilterCard title=\"Semana\" actived={filterActived==='week'} /></button>\n        <button type=\"button\" onClick={()=>setFilterActived(\"month\")}><FilterCard title=\"Mês\" actived={filterActived==='month'} /></button>\n        <button type=\"button\" onClick={()=>setFilterActived(\"year\")}><FilterCard title=\"Ano\" actived={filterActived==='year'} /></button>\n      </S.FilterArea>\n      <S.Title><h3>{filterActived ==='late'? 'TAREFAS ATRASADAS': 'TAREFAS'}</h3></S.Title>\n      <S.Content>\n        {\n          tasks.map(t=>(\n            <Link to={`/task/${t._id}`}>\n                <TaskCard type={t.type} title={t.title} when={t.when} done={t.done}/>\n            </Link>\n            ))\n        }\n       \n      </S.Content>\n        <Footer/></S.Container>\n  );\n}\n\nexport default Home;\n","C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\Header\\index.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\views\\Home\\styles.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\Header\\styles.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\Footer\\index.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\Footer\\styles.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\FilterCard\\index.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\FilterCard\\styles.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\TaskCard\\index.js",["82","83"],"import React,{useMemo} from 'react';\r\nimport {format} from 'date-fns';\r\nimport * as S from './styles';\r\n\r\n\r\nimport typeIcons from '../../utils/typeIcons';\r\n\r\nfunction TaskCard({type,title,when}){\r\n\r\n  const date=useMemo(()=>format(new Date(when),'dd/MM/yyyy'));\r\n  const hour=useMemo(()=>format(new Date(when),'HH:mm'));\r\n\r\n    return  (\r\n        <S.Container>\r\n        <S.TopCard><img src={typeIcons[type]} alt=\"icone da tarefa\"/></S.TopCard>\r\n        <h3>{title}</h3>\r\n        <S.BottomCard>\r\n          <strong>{date}</strong>\r\n          <span>{hour}</span>\r\n        </S.BottomCard>\r\n        </S.Container>\r\n      );\r\n}\r\n\r\nexport default TaskCard;","C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\components\\TaskCard\\styles.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\services\\api.js",[],"C:\\Users\\gabiw\\OneDrive\\Área de Trabalho\\Dev\\web\\web\\src\\utils\\typeIcons.js",["84"],"// icones \r\n\r\nimport iconDefault from '../assets/default.png